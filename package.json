{
  "name": "douyin-trending-crawler",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node crawler-github.js",
    "api": "node api/download.js",
    "dev": "node api/download.js",
    "worker": "node worker/download-worker.js",
    "worker:dev": "DEBUG=true node worker/download-worker.js",
    "clear-data": "node scripts/clear-data.js",
    "test:download": "node scripts/run-download-on-first.js",
    "test:simple": "node scripts/test-simple-download.js",
    "list:viral": "node scripts/list-viral.js",
    "crawl:efficient": "node scripts/efficient-crawler.js",
    "download:smart": "node scripts/smart-download.js",
    "download:top": "npm run download:smart -- --count=10 --days=3",
    "crawl:batch": "npm run crawl:efficient -- --limit=200 && npm run download:smart -- --count=10",
    "pipeline": "node scripts/complete-pipeline.js",
    "pipeline:fast": "npm run pipeline -- --crawl-limit=100 --download-count=5",
    "pipeline:download": "npm run pipeline -- --download-only --count=10 --days=3",
    "pipeline:crawl": "npm run pipeline -- --crawl-only --limit=200",
    "demo": "node scripts/demo-system.js",
    "demo:crawl": "npm run crawl:efficient -- --mock --limit=50",
    "demo:download": "npm run download:smart -- --mock --count=5",
    "demo:full": "npm run demo:crawl && npm run demo:download",
    "debug:douyin": "node scripts/debug-douyin.js",
    "analyze:structure": "node scripts/analyze-douyin-structure.js",
    "crawl": "node scripts/efficient-crawler.js",
    "status": "node scripts/system-status.js",
    "test": "node scripts/test-pipeline.js",
    "summary": "node scripts/final-summary.js",
    "improvements": "node scripts/improvements.js",
    "demo:files": "node scripts/generate-demo-files.js",
    "create:videos": "node scripts/create-real-videos.js",
    "crawl:advanced": "node scripts/advanced-anti-detection.js",
    "extract:real": "node scripts/real-extraction.js",
    "extract:api": "node scripts/api-extraction.js",
    "integrate:alternative": "node scripts/alternative-integration.js",
    "assess": "node scripts/final-assessment.js",
    "download:real": "node scripts/real-video-downloader.js",
    "download:simple": "node scripts/simple-real-downloader.js",
    "extract:urls": "node scripts/video-url-extractor.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "playwright": "^1.40.0"
  }
}
